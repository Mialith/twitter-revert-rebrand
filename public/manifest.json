{
    "manifest_version": 3,
    "name": "Twitter Revert Rebrand",
    "description": "Chirp chirp",
    "version": "1.0.0",
    "icons": {
        "16": "images/twitter-16.png",
        "32": "images/twitter-32.png",
        "64": "images/twitter-64.png",
        "128": "images/twitter-128.png"
    },
    "web_accessible_resources": [{
        "resources": ["images/twitter-32.png"],
        "matches": ["https://twitter.com/*"]
    }],
    "content_scripts": [{
        "js": ["scripts/content.js"],
        "matches": ["https://twitter.com/*"]
    }]
}
